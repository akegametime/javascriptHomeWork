unction findPerimeter(l,w){

let total = (l+w)*2;

return total;


}


function findPerimeter2(l,w){

    let total = (l+w)*2;
    
    return total;
    
    
    }
function findPerimeter3(l,w){

        let total = (l+w)*2;
        
        return total;
        
        
        }
    

console.log (findPerimeter(6,7));
